<form action="/random/results">
  <label for="min_input">Minimum</label>
  <input id="min_input" type="text" name="user_min" placeholder="E.g. 1.5">
  
  <label for="max_input">Maximum</label>
  <input id="max_input" type="text" name="user_max" placeholder="E.g. 10">
  
  <button>Pick random number</button>
</form>
